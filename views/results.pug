ul
  each item in subResults
    li 
      if /^https:/.test(item.data.thumbnail)
        img(src= item.data.thumbnail alt='Reddit thubmnail')
        .result-info-container
          p= item.data.title
          a(href= item.data.url) #{item.data.url}
      else
        div
        .result-info-container
          p= item.data.title
          a(href= item.data.url) #{item.data.url}
